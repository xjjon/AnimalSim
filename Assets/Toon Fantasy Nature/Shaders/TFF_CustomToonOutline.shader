				#endif

				float3 vertexValue = ( input.normalOS * _OutlineWidth * min( unityObjectToClipPos98.w , 1.5 ) );

				#ifdef ASE_ABSOLUTE_VERTEX_POS
					input.positionOS.xyz = vertexValue;
				#else
